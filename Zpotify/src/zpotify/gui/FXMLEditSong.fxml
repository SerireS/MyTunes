<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="400.0" style="-fx-background-color: #222222;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="zpotify.gui.FXMLEditSongController">
   <children>
      <Label layoutX="170.0" layoutY="14.0" text="Edit Song" textFill="WHITE">
         <font>
            <Font name="Lato Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="37.0" layoutY="108.0" text="Titel:" textFill="WHITE">
         <font>
            <Font name="Lato Bold" size="14.0" />
         </font>
      </Label>
      <TextField layoutX="92.0" layoutY="105.0" prefHeight="25.0" prefWidth="268.0" promptText="Enter text" style="-fx-background-color: #333333;" stylesheets="@style/textFieldCss.css" />
