         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btn_save" layoutX="308.0" layoutY="155.0" mnemonicParsing="false" onAction="#handleButtonActionSave" prefHeight="25.0" prefWidth="52.0" stylesheets="@style/buttonCss.css" text="Save" textFill="WHITE">
